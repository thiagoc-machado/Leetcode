class Solution(object):
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        if x < 0:
            return False
        if x == int(str(x)[::-1]):
            #print(x, 'is palindrome.')
            return True
        else:
            #print(x, 'is not palindrome.')
            return False
